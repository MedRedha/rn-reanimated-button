{
  "build": 0,
  "private": true,
  "version": "0.0.1",
  "name": "rntest",
  "homepage": "https://github.com/MedRedha",
  "description": "React Native Upwork Test",
  "author": {
    "name": "Med Redha Khelifi",
    "email": "redha@wuud-team.com",
    "url": "https://github.com/MedRedha/RNTest"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "pod": "npx pod-install",
    "prepare": "husky install",
    "postinstall": "npx jetify",
    "pre-commit": "lint-staged",
    "lint:fix": "eslint --fix src",
    "link": "yarn react-native link",
    "android": "react-native run-android",
    "pod:update": "(cd ios && pod repo update)",
    "ios": "npx react-native run-ios --simulator=\"iPhone Redha Max\"",
    "start": "watchman watch-del-all && watchman watch ./ && npx react-native start --reset-cache",
    "clean:ios": "xcrun simctl erase all && (cd ios && xcodebuild -alltargets clean) && defaults write com.apple.CoreSimulator.IndigoFramebufferServices FramebufferRendererHint 3",
    "clean": "yarn cache clean && watchman watch-del-all && rm -rf /Users/MedRedha/Library/Developer/Xcode/DerivedData/ && xcrun simctl erase all && (cd ios && xcodebuild -alltargets clean) && defaults write com.apple.CoreSimulator.IndigoFramebufferServices FramebufferRendererHint 3"
  },
  "dependencies": {
    "jetifier": "^2.0.0",
    "react": "^18.2.0",
    "react-native": "0.70.6",
    "react-native-animatable": "^1.3.3",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/runtime": "^7.20.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.73.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.js --fix-dry-run"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
